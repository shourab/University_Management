<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
    @RenderSection("style", required: false)

</head>
<body>
    <div class="navbar navbar-inverse navbar-fixed-top">
        <div class="container">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                @*@Html.ActionLink("Application name", "Index", "Home", null, new { @class = "navbar-brand" })*@
            </div>
            <div class="navbar-collapse collapse">
                <ul class="nav navbar-nav">
                    @*<li>@Html.ActionLink("Home", "Index", "Home")</li>
                    <li>@Html.ActionLink("About", "About", "Home")</li>
                    <li>@Html.ActionLink("Contact", "Contact", "Home")</li>*@

                    <li>@Html.ActionLink("1.Department Entry", "Index", "DepartmentGom")</li>
                    <li>@Html.ActionLink("2.View Department", "ShowDepartment", "DepartmentGom")</li>
                    <li>@Html.ActionLink("3.Save Course", "SaveCourse", "DepartmentGom")</li>
                    <li>@Html.ActionLink("4.Save Teacher", "SaveTeacher", "DepartmentGom")</li>


                    <li>@Html.ActionLink("5.Course Assign To Teacher", "SaveCourseAssigntoTeacher", "TeacherAssign")</li>
                    <li>@Html.ActionLink("6.View Course Statistics", "Index", "CourseView")</li>


                    <li>@Html.ActionLink("7.Register Student", "RegisterStudent", "DepartmentGom")</li>
                    <li>@Html.ActionLink("8.Allocate Classroom", "AllocateClassRoomGom", "ResultStudentGOM")</li>
                    <li>@Html.ActionLink("9.View Schedule", "ShowClassSchedule", "ResultStudentGOM")</li>


                    <li>@Html.ActionLink("10.Enroll Course", "Enroll", "EnrollStudentsrb")</li>
                    <li>@Html.ActionLink("11.Save Result", "GenerateResult", "Result")</li>


                    <li>@Html.ActionLink("12.Result", "Index", "ResultStudentGOM")</li>


                    <li>@Html.ActionLink("13.Unassign All Courses", "UnassignCourse", "UnassignCourse")</li>
                    <li>@Html.ActionLink("14.Unassign All Classrooms", "UnassignClassroom", "UnassignCourse")</li>

                </ul>
                @*@Html.Partial("_LoginPartial")*@
            </div>
        </div>
    </div>
    <div class="container body-content">
        @RenderBody()
        <hr />
        <footer>
            @*<p>&copy; @DateTime.Now.Year - My ASP.NET Application</p>*@
        </footer>
    </div>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>
